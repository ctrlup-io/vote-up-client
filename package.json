{
  "name": "vote-up-client",
  "version": "0.1.0",
  "private": true,
  "source": "src/index.html",
  "scripts": {
    "build": "rimraf dist && parcel build",
    "commit": "git-cz",
    "format": "prettier --write {*,src/**/*}.{js,json,md,html}",
    "lint": "eslint --cache .",
    "release:dry": "multi-semantic-release --dry-run",
    "start": "parcel",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,md,html}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "dist"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "@ctrlup/rainbow-react": "^1.1.3",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.16",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@parcel/reporter-bundle-analyzer": "^2.4.0",
    "@qiwi/multi-semantic-release": "^6.0.2",
    "buffer": "^6.0.3",
    "eslint": "^8.11.0",
    "eslint-config-react-app": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "parcel": "^2.4.0",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "sort-package-json": "^1.55.0",
    "typescript": "^4.6.2"
  }
}
